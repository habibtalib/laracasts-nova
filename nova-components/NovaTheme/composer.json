{
    "name": "rushughes/nova-theme",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Rushughes\\NovaTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rushughes\\NovaTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
